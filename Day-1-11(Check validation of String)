#include <stdio.h>
#include <string.h>

int main() {
    char inputString[50];
    printf("192373024_ Akhilkumar\n");
    printf("Enter a string: ");
    scanf("%s", inputString);
    
    if (strlen(inputString) > 0) {
        printf("Valid string entered.\n");
    } else {
        printf("Invalid string entered.\n");
    }
    
    return 0;
}
